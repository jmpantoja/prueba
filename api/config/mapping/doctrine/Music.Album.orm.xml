<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Domain\Music\Album" table="album">
        <id name="id" column="id" type="AlbumId"/>

            <many-to-one field="group" target-entity="App\Domain\Music\MusicGroup"/>

            <field name="name" type="AlbumName"/>
            
            <embedded name="price" class="App\Domain\Music\VO\Money"/>
            
            <field name="releaseAt" type="Year"/>
            
        
    </entity>

</doctrine-mapping>






